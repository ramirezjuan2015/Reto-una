{
  "expo": {
    "name": "Zoo",
    "slug": "zoo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/elite.png",
    "splash": {
      "image": "./assets/slider-tool.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.valentinartv.zoo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/elite.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.valentinartv.zoo"
    },
    "web": {
      "favicon": "./assets/elite.png"
    }
  }
}